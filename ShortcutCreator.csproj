  </ItemGroup>

  <ItemGroup>
    <Content Include="Arrow.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="WindowsAPICodePack" Version="8.0.14" />
  </ItemGroup>

</Project>